{
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "strict": true,
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "."
  },
  "include": ["sanity.config.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}